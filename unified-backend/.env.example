API_KEY=your_secret_api_key_here
PORT=3001
FRONTEND_ORIGIN=http://localhost:8080
DATABASE_URL="file:./prisma/dev.db"
DETECTOR_PROVIDER=multi-ai

# AI Provider Configuration - Replace with your actual API keys
# Ollama (Free local AI - install from https://ollama.ai)
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# DeepSeek (Free tier available - get key from https://platform.deepseek.com)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# OpenRouter (Free models available - get key from https://openrouter.ai)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODEL=mistralai/mistral-7b-instruct

# Hugging Face (Free inference API - get key from https://huggingface.co)
HUGGING_FACE_API_KEY=your_huggingface_api_key_here

# Claude API (Optional - get key from https://console.anthropic.com)
CLAUDE_API_KEY=your_claude_api_key_here

# GPT-4 Vision (Optional - get key from https://platform.openai.com)
OPENAI_API_KEY=your_openai_api_key_here

# Production Settings
RATE_LIMIT_ENABLED=true
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100